{
  "exclude": ["node_modules", "dist"],
  "compilerOptions": {
    "outDir": "dist",
    "noEmit": false,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true
  }
}
